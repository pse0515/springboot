<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.korit.springboot.mapper.StudyMapper">

    <insert id="insert">
        insert into study_tb
        values (default, #{name}, #{age})
    </insert>

    <select id="findAllName" resultType="string">
        select
            name
        from
            study_tb
    </select>

    <select id="findAll" resultType="java.util.Map">
        select
            study_id as studyId,
            name,
            age
        from
            study_tb
    </select>
</mapper>